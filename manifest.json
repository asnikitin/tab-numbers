{
  "manifest_version": 2,
  "name": "Numbered Tabs",
  "description": "See order of your open tabs to navigate easily",
  "version": "0.1",
  "permissions": [
    "file://*/*",
    "http://*/*",
    "https://*/*",
    "tabs"
  ],
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  }
}
